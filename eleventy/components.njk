            id="story-{{ story | lower }}"
            path="{{ component.path }}" 
            tag="{{ customElements | getTagName(component.name) }}" 
            key="{{ story }}"
            {% if story === 'Interactive' %}interactive{% endif %} 
        >
        </story-renderer>
        {%- endfor %}
        <nav style="position: sticky; top: 84px; background-color: pink; flex: 0 0 240px;">
