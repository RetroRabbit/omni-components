---
title: Omni Components - {{ component.name }}
layout: page.njk
pagination:
    data: components
    size: 1
    alias: component
permalink: "components/{{ component.name | slug }}/"
---

<div>{{ component.name }}</div> 
<div>{{ customElements | getTagName(component.name) }}</div> 
<div>{{ customElements | getDescription(component.name) | safe }}</div> 

<!-- Tab links -->
<div class="tab">
  <button class="tablinks active" onclick="openTab(event, 'docs')">Docs</button>
  <button class="tablinks" onclick="openTab(event, 'props')">CSS Props</button>
</div>

<!-- Tab content -->
