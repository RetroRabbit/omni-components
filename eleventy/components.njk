---
title: Omni Components
layout: page.njk
pagination:
    data: components
    size: 1
    alias: component
permalink: "components/{{ component.name | slug }}/"
---

<div>{{ customElements | getTagName(component.name) }}</div> 
<div>{{ customElements | getDescription(component.name) }}</div> 

<story-renderer path="{{ component.path }}" tag="{{ customElements | getTagName(component.name) }}"></story-renderer>